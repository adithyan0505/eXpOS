// load INIT from disk to memory
loadi(65,7);
loadi(66,8);
// load INT 10 from disk to memory
loadi(22,35);
loadi(23,36);
// load exception handler routine
loadi(2,15);
loadi(3,16);
// setup page table for INIT
PTBR = PAGE_TABLE_BASE;
PTLR = 9;
[PTBR+8] = 65;
[PTBR+9] = "0100";
[PTBR+10] = 66;
[PTBR+11] = "0100";
[PTBR+16] = 76;
[PTBR+17] = "0110";
// set IP to start of code when mode changes from kernel to user
[76 * 512] = 4 * 512;
SP =  8 * 512;
ireturn;
