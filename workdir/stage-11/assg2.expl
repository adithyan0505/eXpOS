type
	Student {
		str name;
		int marks;
	}
endtype

int main() {
	decl
		int temp;
		Student s;
	enddecl
	begin
		temp = initialize();
		s = alloc();
		if(s != null) then
			s.name = "John";
			s.marks = 89;

			temp = exposcall("Write", -2, s.name);
			temp = exposcall("Write", -2, s.marks);
		endif;
		free(s);
		return 0;
	end
}

