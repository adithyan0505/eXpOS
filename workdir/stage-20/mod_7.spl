// load INIT from disk to memory
loadi(65,7);
loadi(66,8);
// load INT 10 from disk to memory
loadi(22,35);
loadi(23,36);
// load exception handler routine
loadi(2,15);
loadi(3,16);
// load timer interrupt routine
loadi(4,17);
loadi(5,18);
// load INT 7 from disk to memory
loadi(16,29);
loadi(17,30);
// load MODULE_5 (scheduler)
loadi(50,63);
loadi(51,64);
// load MODULE_4 (Device Manager)
loadi(48,61);
loadi(49,62);
// load MODULE_0 (Resource Manager)
loadi(40,53);
loadi(41,54);
// load console interrupt routine
loadi(8,21);
loadi(9,22);
// load INT 6
loadi(14,27);
loadi(15,28);
// load INT 9
loadi(20,33);
loadi(21,34);
// load MODULE_1 (Process Manager)
loadi(42,55);
loadi(43,56);
// load MODULE_2 (Memory Manager)
