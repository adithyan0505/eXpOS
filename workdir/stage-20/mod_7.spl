// load INT 8
loadi(18,31);
loadi(19,32);

alias process_table R0; 

// INIT process (PID 1)

// SETUP PAGE TABLE
PTBR = PAGE_TABLE_BASE + 20;
PTLR = 10;
//Library
[PTBR+0] = 63;
[PTBR+1] = "0100";
[PTBR+2] = 64;
[PTBR+3] = "0100";
//Heap
[PTBR+4] = 78;
[PTBR+5] = "0110";
[PTBR+6] = 79;
[PTBR+7] = "0110";
//Code
[PTBR+8] = 65;
[PTBR+9] = "0100";
[PTBR+10] = 66;
[PTBR+11] = "0100";
[PTBR+12] = -1;
[PTBR+13] = "0000";
[PTBR+14] = -1;
[PTBR+15] = "0000";
//Stack
[PTBR+16] = 76;
[PTBR+17] = "0110";
[PTBR+18] = 77;
[PTBR+19] = "0110";

// SETUP PROCESS TABLE
process_table = PROCESS_TABLE + 16;
//PID, UAPN, UPTR, KPTR, PTBR, PTLR, STATUS
[process_table+1] = 1;
[process_table+11] = 80;
[process_table+13] = 8*512;
[process_table+12] = 0;
[process_table+14] = PTBR;
[process_table+15] = PTLR;
[process_table+4] = CREATED;

// set IP to start of code when mode changes from kernel to user
[76 * 512] = [65 * 512 + 1];

// Initialize PER-PROCESS RESOURCE TABLE for INIT
alias t R1;
alias resource_table R2;
t = 0;
resource_table = [process_table + 11] * 512 + RESOURCE_TABLE_OFFSET;
while(t < 16) do
	[resource_table + t] = -1;
	t = t + 1;
endwhile;

// Initialize PER-PROCESS DISK MAP TABLE for INIT
[DISK_MAP_TABLE + 10 + 0] = -1;
[DISK_MAP_TABLE + 10 + 1] = -1;
[DISK_MAP_TABLE + 10 + 2] = -1;
[DISK_MAP_TABLE + 10 + 3] = -1;
[DISK_MAP_TABLE + 10 + 4] = 7;
[DISK_MAP_TABLE + 10 + 5] = 8;
[DISK_MAP_TABLE + 10 + 6] = -1;
[DISK_MAP_TABLE + 10 + 7] = -1;
[DISK_MAP_TABLE + 10 + 8] = -1;
[DISK_MAP_TABLE + 10 + 9] = -1;

// Initialize STATE field for remaining entries in PROCESS TABLE
t = 2;
while(t < 16) do
	process_table = process_table + 16;	
	[process_table+4] = TERMINATED;
	t = t + 1;
endwhile;

// Initialize STATUS in Terminal Status Table
[TERMINAL_STATUS_TABLE] = 0;

// Initialize Memory Free List
t = 0;
while(t < 83) do
	[MEMORY_FREE_LIST + t] = 1;
	t = t + 1;
endwhile;
while(t < MAX_MEM_PAGE) do
	[MEMORY_FREE_LIST + t] = 0;
	//increment MEM_FREE_COUNT
	[SYSTEM_STATUS_TABLE + 2] = [SYSTEM_STATUS_TABLE + 2] + 1;
	t = t + 1;
endwhile;
// Initialize WAIT_MEM_COUNT
[SYSTEM_STATUS_TABLE + 3] = 0;

// Initialize STATUS in Disk Status Table
[DISK_STATUS_TABLE] = 0;

// return to caller (OS startup code)
return;
